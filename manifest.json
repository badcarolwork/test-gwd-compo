  ],
  "attributes": [
    {
      "name": "leftimg",
      "label": "Left Image",
      "type": "file",
      "asset": true,
      "required": true,
      "description": "upload left image",
      "accept": "image/*"
    },
    {
      "name": "resultleftimg",
      "label": "Left Result Image",
      "type": "file",
      "asset": true,
      "required": true,
      "description": "upload left result image",
      "accept": "image/*"
    },
    {
      "name": "lefturl",
      "label": "Left Landing URL",
      "type": "string",
      "required": true,
      "description": "Landing url for left"
    },
    {
      "name": "rightimg",
      "label": "Right Image",
      "type": "file",
      "asset": true,
      "required": true,
      "description": "upload right image",
      "accept": "image/*"
    },
    {
      "name": "resultrightimg",
      "label": "Right Result Image",
      "type": "file",
      "asset": true,
      "required": true,
      "description": "upload right result image",
      "accept": "image/*"
    },
    {
      "name": "righturl",
      "label": "Right Landing URL",
      "type": "string",
      "required": true,
      "description": "Landing url for right"
    },
    {
      "name": "barcolor",
      "label": "Bar Color",
      "type": "string",
      "defaultValue": "#ffffff",
      "description": "Color code for the handler bar"
    },
    {
      "name": "barwidth",
      "label": "Bar Width",
      "type": "string",
      "defaultValue": "4",
      "description": "The line width of the bar"
    },
    {
      "name": "clickfld",
      "label": "Click FLD",
      "type": "string",
      "description": "click floodlight full"
    }
