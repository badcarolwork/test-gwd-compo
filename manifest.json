{
  "name": "PFX Cross Slider",
  "type": "pfx-crossslider",
  "tagName": "pfx-crossslider",
  "version": 1.03,
  "customElementsVersion": 1,
  "description": "Performics Cross Slider",
  "externalScripts": [
    "https://s0.2mdn.net/creatives/assets/4671853/gsap.min.js",
